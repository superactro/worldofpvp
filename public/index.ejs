<!DOCTYPE html>
<html>
<head>
    <title>World of PvP</title>
    <meta name="viewport" content="width=device-width">
    <meta name="language" content="en"> 
    <meta name="description" content="Insane multiplayer shooter! Play vs friends, create new levels.">
    <meta name="keywords" content="shooter, platformer, multiplayer, pvp, create levels, social, game, beta, facebook">
    <!-- <link href='//fonts.googleapis.com/css?family=Audiowide' rel='stylesheet' type='text/css'> -->
    <link rel="icon" href="media/favicon.png" type="image/x-icon"/>
    <link rel="stylesheet" href="/css/style.css">
    <script src="lib/jquery.js"></script>

    <script>
        ImpactPrefix = 'impact/';
        ClientHost = 'http://<%= config.host %>:<%= config.port %>';
    </script>
    <script type="text/javascript" src="http://<%= config.host %>:<%= config.port %>/socket.io/socket.io.js"></script>
    <script type="text/javascript" src="impact/lib/impact/impact.js"></script>
    <script type="text/javascript" src="impact/lib/game/main.js"></script>

    <script>
    if (document.location.hostname == "localhost") {
      console.log("local Server");
          $.getScript( "lib/fconnect.js", function() {});
    }
    else {
      console.log("cloud Server");
          $.getScript( "lib/fconnect.js", function() {});
    }
  </script>

</head>
<body id="body">

    <audio id="btnHover">
        <source src="media/sounds/btnHover.mp3"></source>
        <source src="media/sounds/btnHover.ogg"></source>
    </audio>

    <!-- facebook fb-root div -->
    <div id="fb-root"></div>


    <!-- game main interface container -->

    <!-- game main interface container -->
    <div class="homeContainer">
        <div class="row row1 statsRow">
            <div class="expirience">
                <div class="expirienceFull"></div>
            </div>
            <div class="col ">
                <div id="playerName"></div>
                <a id="profileImg" href="#">
                </a>
            </div>
            <div class="col colSmall">
                <span class="smallFont">level</span> 
                <span class="mediumFont">1</span>
            </div>
            <div class="col colSmall">
                <span class="smallFont">gold</span> 
                <span class="mediumFont">0</span>
            </div>
            <div class="col colSmall">
                <span class="smallFont">total wins</span> 
                <span class="mediumFont">0</span>
            </div>
            <div class="col colSmall">
                <span class="smallFont">kills</span> 
                <span class="mediumFont">0</span>
            </div>
            <div class="col colSmall">
                <span class="smallFont">killstreak</span> 
                <span class="mediumFont">0</span>
            </div>
            <div class="col colSmall">
                <span class="smallFont">winstreak</span> 
                <span class="mediumFont">0</span>
            </div>
        </div>
        <!-- End row1 statsRow-->

        <div class="row row2 armyRow">
            <div class="col ">
                <span class="smallFont friendName1"></span> 
                <a class="friendImgBox1" href="#"></a>
            </div>
            <div class="col ">
                <span class="smallFont friendName2"></span> 
                <a class="friendImgBox2" href="#"></a>
            </div>
            <div class="col ">
                <span class="smallFont friendName3"></span> 
                <a class="friendImgBox3" href="#"></a>
            </div>
            <div class="col ">
                <span class="smallFont friendName4"></span> 
                <a class="friendImgBox4" href="#"></a>
            </div>
            <div class="col ">
                <span class="smallFont friendName5"></span> 
                <a class="friendImgBox5" href="#"></a>
            </div>
            <div class="col ">
                <span class="smallFont friendName6"></span> 
                <a class="friendImgBox6" href="#"></a>
            </div>
            <div class="col ">
                <span class="smallFont friendName7"></span> 
                <a class="friendImgBox7" href="#"></a>
            </div>

        </div>
        <!-- End row3 armyRow-->

        <div class="row row3 questRow">
            <div class="col ">
                <span class="smallFont">50$</span> 
                <a href="#"><img class="playerImg" src="media/demo/grenade.png" alt="demo profile image"></a>
            </div>
            <div class="col">
                <span class="smallFont">100$</span> 
                <a href="#"><img class="playerImg" src="media/demo/pistol.png" alt="demo profile image"></a>
            </div>
            <div class="col">
                <span class="smallFont">300$</span> 
                <a href="#"><img class="playerImg" src="media/demo/uzi.png" alt="demo profile image"></a>
            </div>
            <div class="col">
                <span class="smallFont">500$</span> 
                <a href="#"><img class="playerImg" src="media/demo/ak47.png" alt="demo profile image"></a>
            </div>
            <div class="col">
                <span class="smallFont">1000$</span> 
                <a href="#"><img class="playerImg" src="media/demo/sniper.png" alt="demo profile image"></a>
            </div>
            <div class="col">
                <span class="smallFont">1400$</span> 
                <a href="#"><img class="playerImg" src="media/demo/law.png" alt="demo profile image"></a>
            </div>
            <div class="col">
                <span class="smallFont">2000$</span> 
                <a href="#"><img class="playerImg" src="media/demo/gravity.png" alt="demo profile image"></a>
            </div>
        </div>
        <!-- End row3 questRow-->

        <div class="stats">
            <a src="#" class="mainStats">login</a>
        </div>

    </div><!-- end homeContainer -->
    <!-- End game main interface container -->

    <canvas id="canvas"></canvas>
    <img class="logo" width="400" src="media/logo.png" alt="worldofpvp">
    <!-- <script src="js/noRight.js"></script> -->
    <script src="lib/interface.js"></script>

    <!-- Google Analitics -->
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
            (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
            m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-42935839-1', 'worldofpvp.com');
        ga('send', 'pageview');
    </script>
</body>
</html>
